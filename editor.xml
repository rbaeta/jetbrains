<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="18" />
    <option name="FONT_FAMILY" value="Source Code Pro" />
    <option name="FONT_SCALE" value="1.25" />
    <option name="LINE_SPACING" value="1.3" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="XML" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_FOLDING_OUTLINE_SHOWN" value="false" />
    <option name="IS_DND_ENABLED" value="false" />
    <option name="REFRAIN_FROM_SCROLLING" value="true" />
  </component>
  <component name="InspectionProfileManager">
    <info color="bc">
      <option name="EFFECT_COLOR" value="bc" />
      <option name="ERROR_STRIPE_COLOR" value="bc" />
      <option name="EFFECT_TYPE" value="2" />
      <option name="myName" value="Check Warning" />
      <option name="myVal" value="50" />
      <option name="myExternalName" value="Check Warning" />
      <option name="myDefaultAttributes">
        <option name="ERROR_STRIPE_COLOR" value="bc" />
      </option>
    </info>
    <list size="8">
      <item index="0" class="java.lang.String" itemvalue="INFORMATION" />
      <item index="1" class="java.lang.String" itemvalue="TYPO" />
      <item index="2" class="java.lang.String" itemvalue="Check Warning" />
      <item index="3" class="java.lang.String" itemvalue="SERVER PROBLEM" />
      <item index="4" class="java.lang.String" itemvalue="WEAK WARNING" />
      <item index="5" class="java.lang.String" itemvalue="INFO" />
      <item index="6" class="java.lang.String" itemvalue="WARNING" />
      <item index="7" class="java.lang.String" itemvalue="ERROR" />
    </list>
  </component>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="//\btodo\b.*" />
    <pattern pattern="//\bfixme\b.*" />
    <pattern case-sensitive="true" pattern="//\brb\b.*" />
    <filter name="my comments">
      <pattern index="2" />
    </filter>
  </component>
</application>